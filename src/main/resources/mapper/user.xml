<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.polarBear.Mapper.UserMapper">
	
	<!-- 리스트 카운트 -->
	<select id="selectUserOne" resultType="com.polarBear.Dto.UserDto">
		SELECT 
			*
		FROM 
			us_user
		WHERE
			login_id = #{loginId}
	</select>
	
</mapper>